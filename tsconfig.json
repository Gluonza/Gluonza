{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext.Intl",
      "ES2022.Intl"
    ],
    "jsx": "react",
    "experimentalDecorators": true,
    "jsxFactory": "__jsx__.createElement",
    "jsxFragmentFactory": "__jsx__.Fragment",
    "module": "ESNext",
    "moduleResolution": "Node10",
    "paths": {
      "common": [
        "./common"
      ],
      "common/*": [
        "./common/*"
      ],
      "typings": [
        "./typings"
      ],
      "typings/*": [
        "./typings/*"
      ]
    },
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types",
      "./typings/index.d.ts"
    ]
  }
}
